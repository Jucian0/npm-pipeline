{
  "name": "autoversion",
  "version": "5.0.1",
  "description": "Auto version example using Bitbucket Pipelines",
  "main": "api.js",
  "dependencies": {},
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-git": "^2.10.1"
  },
  "scripts": {
    "tag:develop": "gulp up-develop --gulpfile ./scripts/gulpfile.js",
    "tag:patch": "gulp up-patch --gulpfile ./scripts/gulpfile.js",
    "tag:minor": "gulp up-minor --gulpfile ./scripts/gulpfile.js",
    "tag:major": "gulp up-major --gulpfile ./scripts/gulpfile.js"
  },
  "author": "Shane Fast",
  "license": "MIT"
}